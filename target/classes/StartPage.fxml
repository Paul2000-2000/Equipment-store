<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.control.cell.*?>
<?import javafx.collections.*?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<GridPane
        fx:id = "StartPage"



        xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" prefHeight="400" prefWidth="600"
          vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal"
          fx:controller="org.loose.fis.proiect.controllers.StartPageController">

    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="17.0"/>
    </padding>


    <HBox alignment="CENTER" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="0">
        <Button fx:id="registrationbutton" textAlignment="CENTER" onAction="#handleRegistrationAction" text="Registration" prefWidth="200" prefHeight="80">
        <font>
            <Font size="30.0"/>
        </font>
        </Button>
    </HBox>



    <HBox alignment="CENTER" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="6">
<Button fx:id="signinbutton" textAlignment="CENTER" onAction="#handleSignInAction" text="Sign in"  prefWidth="200" prefHeight="80">
    <font>
        <Font size="30.0"/>
    </font>
</Button>
</HBox>


    <HBox alignment="CENTER" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="8">
        <Button fx:id="EXIT" textAlignment="CENTER" onAction="#handleEXITAction" text="EXIT"  prefWidth="50" prefHeight="25">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
    </HBox>


</GridPane>
